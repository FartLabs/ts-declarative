{
  "lock": false,
  "name": "@fartlabs/declarative",
  "version": "0.0.12",
  "exports": {
    ".": "./lib/declarative/mod.ts",
    "./common/google-aip": "./lib/declarative/common/google-aip/mod.ts",
    "./common/google-aip/methods": "./lib/declarative/common/google-aip/methods/mod.ts",
    "./common/json-schema": "./lib/declarative/common/json-schema/mod.ts",
    "./common/json-schema/ajv": "./lib/declarative/common/json-schema/ajv/mod.ts",
    "./common/json-schema/auto-schema": "./lib/declarative/common/json-schema/auto-schema/mod.ts",
    "./common/jsonld": "./lib/declarative/common/jsonld/mod.ts",
    "./common/openapi": "./lib/declarative/common/openapi/mod.ts",
    "./common/openapi/oazapfts": "./lib/declarative/common/openapi/oazapfts/mod.ts",
    "./common/type-info": "./lib/declarative/common/type-info/mod.ts"
  },
  "imports": {
    "#/": "./",
    "@comunica/query-sparql-link-traversal": "npm:@comunica/query-sparql-link-traversal@^0.6.1",
    "@deno/doc": "jsr:@deno/doc@^0.169.0",
    "@deno/graph": "jsr:@deno/graph@^0.89.2",
    "@eta-dev/eta": "jsr:@eta-dev/eta@^3.5.0",
    "@oazapfts/runtime": "npm:@oazapfts/runtime@^1.0.4",
    "@sinclair/typemap": "npm:@sinclair/typemap@^0.9.1",
    "@std/assert": "jsr:@std/assert@^1.0.11",
    "@std/collections": "jsr:@std/collections@^1.0.10",
    "@std/fs": "jsr:@std/fs@^1.0.14",
    "@std/http": "jsr:@std/http@^1.0.13",
    "@std/testing": "jsr:@std/testing@^1.0.9",
    "@std/text": "jsr:@std/text@^1.0.12",
    "@types/jsonld": "npm:@types/jsonld@^1.5.15",
    "@wei/pluralize": "jsr:@wei/pluralize@^8.0.2",
    "ajv": "npm:ajv@^8.17.1",
    "jsonld": "npm:jsonld@^8.3.3",
    "oazapfts": "npm:oazapfts@^6.2.2",
    "openapi-types": "npm:openapi-types@^12.1.3",
    "ts-morph": "npm:ts-morph@^25.0.1"
  },
  "tasks": {
    "outdated": "deno outdated --update --latest",
    "test": "deno test --allow-all --unstable-kv",
    "test:update": "deno task test -- --update",
    "lint": "deno lint && deno doc --lint  **/*.ts",
    "check": "deno check **/*.ts",
    "publish:dry": "deno publish --dry-run --allow-dirty",
    "dev": {
      "description": "Runs all development tasks",
      "command": "deno fmt",
      "dependencies": ["check", "test", "lint", "publish:dry"]
    },
    "example": "deno -A examples/getting-started/main.ts",
    "example:alpaca": "deno -A examples/alpaca/main.ts"
  },
  "fmt": {
    "exclude": ["./examples/alpaca/index.eta.html"]
  }
}
